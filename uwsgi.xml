<uwsgi>
    <socket>/tmp/mozbuzzdjangoapp.socket</socket>
    <master/>
    <workers>2</workers>
    <pidfile>/var/lib/mozbuzz/uwsgi.pid</pidfile>
    <pythonpath>/var/lib/mozbuzz/</pythonpath>
    <pythonpath>/var/lib/</pythonpath>
    <pythonpath>/var/www/</pythonpath>
    <pythonpath>..</pythonpath>
    <chdir>/var/lib/mozbuzz/</chdir>
    <env>DJANGO_SETTINGS_MODULE=mozbuzz.settings</env>
    <module>django.core.handlers.wsgi:WSGIHandler()</module>
    <daemonize>true</daemonize>
    <uid>33</uid>
</uwsgi>
